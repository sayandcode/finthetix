{
  "name": "",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "remix build && esbuild server/index.ts --bundle --platform=node --target=node20.10 --outfile=dist/lambda.cjs",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "compile-contracts": "cd ../../contracts && forge build",
    "generate-abi-types": "typechain --target ethers-v6 --out-dir \"app/contracts/types\" \"../../contracts/out/**/*.json\"",
    "postinstall": "npm run compile-contracts && npm run generate-abi-types"
  },
  "dependencies": {
    "@fontsource-variable/cairo": "^5.0.18",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@reduxjs/toolkit": "^2.1.0",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/express": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@types/aws-lambda": "^8.10.136",
    "chart.js": "^4.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "ethers": "^6.11.1",
    "express": "^4.18.3",
    "isbot": "^4.1.0",
    "lucide-react": "^0.316.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "serverless-http": "^3.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.1",
    "@stylistic/eslint-plugin": "^1.5.4",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "autoprefixer": "^10.4.17",
    "esbuild": "^0.20.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "typechain": "^8.3.2",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}