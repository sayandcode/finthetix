{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "preinstall": "cd ../src && npm ci",
    "build": "cd ../src && npm run build",
    "predeploy": "npm run build",
    "predeploy:ci": "npm run build",
    "deploy": "cdk deploy",
    "deploy:ci": "cdk deploy --require-approval never",
    "undeploy": "cdk destroy",
    "lint": "eslint . --ext '.ts' --ignore-path .gitignore"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "aws-cdk": "2.133.0",
    "eslint": "^8.57.0",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.133.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  }
}
